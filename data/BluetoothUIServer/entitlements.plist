<!-- Entitlements for BluetoothUIServer.app -->

<?xml version='1.0' encoding='utf-8'?>
<plist version="1.0">
  <dict>
    <key>com.apple.bluetooth.nsxpc</key>
    <true />
    <key>com.apple.bluetooth.system</key>
    <true />
    <key>com.apple.private.bmk.allow</key>
    <true />
    <key>com.apple.private.notificationcenter-system</key>
    <array>
      <dict>
        <key>identifier</key>
        <string>com.apple.BluetoothUIServer</string>
      </dict>
    </array>
    <key>com.apple.private.tcc.allow</key>
    <array>
      <string>kTCCServiceAccessibility</string>
      <string>kTCCServiceBluetoothAlways</string>
    </array>
  </dict>
</plist>